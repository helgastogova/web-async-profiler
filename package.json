{
  "name": "web-async-profiler",
  "version": "0.1.0",
  "main": "main.js",
  "scripts": {
    "dev-next": "next dev",
    "dev-all-electron": "concurrently \"npm run dev\" \"npm run server\" \"npm run electron\"",
    "dev-all": "concurrently \"npm run dev\" \"npm run server\"",
    "dev": "ts-node src/server/server.ts",
    "build": "next build",
    "start": "NODE_ENV=production ts-node src/server/server.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "server": "node src/server/index.ts",
    "electron": "electron .",
    "electron-build": "electron-builder"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "@types/node": "20.6.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "axios": "^1.5.0",
    "classnames": "^2.3.2",
    "d3": "^7.8.5",
    "d3-hierarchy": "^3.1.2",
    "date-fns": "^2.30.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "formik": "^2.4.5",
    "install": "^0.13.0",
    "json-server": "^0.17.3",
    "next": "13.4.19",
    "next-auth": "^4.23.2",
    "npm": "^10.1.0",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-truncate-markup": "^5.1.2",
    "typescript": "5.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "electron": "^26.2.1",
    "electron-builder": "^24.6.4",
    "ts-node": "^10.9.1"
  }
}
